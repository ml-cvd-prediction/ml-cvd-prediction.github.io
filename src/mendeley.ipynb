    "print(f\"thal mean: {thal_mean}\")\n",
    "\n",
    "# Fill missing values\n",
    "df['noofmajorvessels'].fillna(noofmajorvessels_mean, inplace=True)\n",
    "df['thal'].fillna(thal_mean, inplace=True)"
